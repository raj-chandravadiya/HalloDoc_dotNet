@{    Layout = "_patientDashboardHeader";}<div class="px-5">    <div class="tab-pane fade pt-2 active show justify-content-center" id="home" role="tabpanel" aria-labelledby="home-tab">        <h2 class="mt-5">Medical History</h2>        <div class="border-2 shadow-lg mt-5">            <div class="d-flex  justify-content-end ">                <button onclick="showModal()" class="me-3 mt-3 btn btn-outline-info createNewRequest">                    Create New Request                </button>            </div>            <div class="table-responsive">                <table>                    <colgroup>                        <col  style="width: 30%;">                        <col  style="width: 50%;">                        <col  style="width: 20%;">                    </colgroup>                    <thead>                        <tr>                            <th>                                Created Date
                            </th>                            <th>                                status                            </th>                            <th>                                Document                            </th>                        </tr>
                    </thead>                    <tbody>                        @foreach(var val in Model)
                        {
                            <tr>
                                <td>
                                    @val.date
                                </td>                                <td>                                    @val.status                                </td>                                <td>                                    <a asp-controller="patientDash" asp-action="viewDocuments" class="btn btn-info">                                        Document                                    </a>                                </td>                            </tr>
                        }                    </tbody>                </table>            </div>        </div>    </div></div><div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">    <div class="modal-dialog modal-dialog-centered" role="document">        <div class="modal-content">            <div class="modal-header bg-info">                <h5 class="modal-title text-white" id="exampleModalLongTitle">Create New Request</h5>                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>            </div>            <form>                <div class="modal-body">                    <span>Here, I want to create new request for ...</span>                    <div class="modal-flex d-flex justify-content-center my-3">                        <div class="btn create-request-active create-request-btn border border-1 border-info" onclick="toggle(this)"> Me </div>                        <a id="my_request_link"  href="@Url.Action("patientInformationByMe","PatientDash",new{userid =15})" hidden></a>                        <div class="btn ms-2 border border-1 someone-else-btn create-request-btn border-info" onclick="toggle(this)"> Some One Else</div>                        <a id="someone_else_link" asp-controller="patientDash" asp-action="patientInformationBySomeone" hidden></a>                    </div>                </div>                <div class="modal-footer">                    <button type="button" class="btn btn-info text-white" data-bs-dismiss="modal" onclick="redirect()">Continue</button>                    <button type="button" class="btn cancel-btn" data-bs-dismiss="modal">Cancel</button>                </div>            </form>        </div>    </div></div>@section createRequestModal {    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>     <script>        function showModal() {            $('#myModal').modal('show');        }            var isMeRequest = true;        function toggle(element) {            if (element.classList.contains("someone-else-btn")) {                isMeRequest = false;            }            else {                isMeRequest = true;            }            if (!element.classList.contains("create-request-active")) {                $(".create-request-btn").removeClass("create-request-active");                element.classList.add("create-request-active");            }        }        function redirect() {            if (isMeRequest) {                document.getElementById("my_request_link").click();            }            else {                document.getElementById("someone_else_link").click();            }        }    </script>}